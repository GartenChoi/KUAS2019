<!--// @todo 여기부터 페이지마다 달라야 함.-->
<!--오픈그래프-->
<meta property="og:title" content="<%-locals.title%>">
<meta property="og:url" content="<%-locals.url%>">
<meta property="og:description" content="<%-locals.description%>">
<%locals.images.forEach(function(item){%>
<meta property="og:image" content="<%-fullURL(item)%>">
<%})%>
<!--SEO-->
<meta name="description" lang="ko" content="<%-locals.description%>">
<meta name="keywords" lang="ko" content="<%-locals.keywords.join(',')%>">
<link rel="canonical" lang="ko" content="<%-locals.url%>">
<title><%-locals.title%></title>
<!--애플 모바일웹 앱 제목. -->
<meta name="apple-mobile-web-app-title" content="<%-locals.title%>">

<!--스키마-->
<script type="application/ld+json">
    <%-JSON.stringify(locals.schema)%>
</script>
