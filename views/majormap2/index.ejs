<script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
<script>
    $(document).ready(function(){
        $('.index--banner--container .contents-area').owlCarousel({
            loop:true,
            nav:true
        })
    })
</script>
</body>
</html>
